{
	"build": {
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build",
		"devPath": "http://localhost:3000",
		"distDir": "../dist",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "tempsbdn",
		"version": "1.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"shell": {
				"all": false,
				"open": true
			}
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "tempsbdn.vercel.app",
			"icon": [
				"icons/badalona.svg",
				"icons/badalona.icns",
				"icons/badalona.ico"
			]
		},
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "tempsbdn",
				"width": 800,
				"height": 600
			}
		]
	}
}
